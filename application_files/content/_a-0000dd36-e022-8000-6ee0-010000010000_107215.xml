<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<contentHaul xmlns:a="http://www.appian.com/ae/types/2009">
    <rule>
        <name>ST_First_Project_Edit_Link</name>
        <uuid>_a-0000dd36-e022-8000-6ee0-010000010000_107215</uuid>
        <description>contains edit link</description>
        <parentUuid>_a-0000dce9-678b-8000-6ee0-010000010000_103022</parentUuid>
        <visibility>
            <advertise>false</advertise>
            <hierarchy>true</hierarchy>
            <indexable>true</indexable>
            <quota>false</quota>
            <searchable>true</searchable>
            <system>false</system>
            <unlogged>false</unlogged>
        </visibility>
        <definition>#&quot;SYSTEM_SYSRULES_submitLink&quot;(
  label:&quot;&quot;,
  value:&quot;Edit&quot;,
  saveInto:{
    a!save(
      ri!action,
      save!value
    ),
  
  a!save(ri!isEdit,true),
  a!save(ri!selectedQuestionId,ri!questionId)
  }
)</definition>
        <namedTypedValue>
            <name>action</name>
            <type>
                <name>string</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
            <value/>
        </namedTypedValue>
        <namedTypedValue>
            <name>isEdit</name>
            <type>
                <name>boolean</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>questionId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <namedTypedValue>
            <name>selectedQuestionId</name>
            <type>
                <name>int</name>
                <namespace>http://www.w3.org/2001/XMLSchema</namespace>
            </type>
        </namedTypedValue>
        <preferredEditor>legacy</preferredEditor>
        <offlineEnabled>false</offlineEnabled>
    </rule>
    <roleMap public="true">
